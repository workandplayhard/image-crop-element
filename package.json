{
  "name": "image-crop-element",
  "version": "1.0.0",
  "description": "Select area for cropping an image. This does not actually crop.",
  "main": "dist/index-umd.js",
  "scripts": {
    "prebuild": "npm run clean && mkdir dist",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "test": "eslint index.js",
    "build": "npm run build-umd",
    "build-umd": "BABEL_ENV=umd babel index.js -o dist/index-umd.js"
  },
  "keywords": [
    "crop",
    "image",
    "custom element"
  ],
  "author": "Mu-An Chiou",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muan/image-crop-element.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.10.0",
    "eslint-plugin-github": "^0.19.0"
  }
}
