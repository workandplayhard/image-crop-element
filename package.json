{
  "name": "@github/image-crop-element",
  "version": "1.0.7",
  "description": "Select area for cropping an image. This does not actually crop.",
  "main": "dist/index-umd.js",
  "files": [
    "dist",
    "index.css"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint index.js",
    "prebuild": "npm run clean && npm run lint && mkdir dist",
    "build-umd": "BABEL_ENV=umd babel index.js -o dist/index.umd.js",
    "build-esm": "BABEL_ENV=esm babel index.js -o dist/index.esm.js",
    "build": "npm run build-umd && npm run build-esm",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "crop",
    "image",
    "custom element"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github/image-crop-element.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-plugin-github": "^0.19.0"
  }
}
