{
  "name": "@github/image-crop-element",
  "version": "4.0.0",
  "description": "Select area for cropping an image. This does not actually crop.",
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "github-lint",
    "prebuild": "npm run clean && npm run lint && mkdir dist",
    "build": "tsc && cp src/index.css dist/index.css",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "karma start test/karma.config.cjs",
    "postpublish": "npm publish --ignore-scripts --@github:registry='https://npm.pkg.github.com'"
  },
  "keywords": [
    "crop",
    "image",
    "custom element"
  ],
  "license": "MIT",
  "prettier": "@github/prettier-config",
  "repository": "github/image-crop-element",
  "devDependencies": {
    "@github/prettier-config": "0.0.4",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-plugin-github": "^3.4.0",
    "karma": "^5.0.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^7.1.2",
    "typescript": "^3.9.3"
  }
}
