{
  "name": "@github/image-crop-element",
  "version": "2.0.0",
  "description": "Select area for cropping an image. This does not actually crop.",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "index.css"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "github-lint",
    "prebuild": "npm run clean && npm run lint && mkdir dist",
    "build-umd": "BABEL_ENV=umd babel index.js -o dist/index.umd.js",
    "build-esm": "BABEL_ENV=esm babel index.js -o dist/index.esm.js",
    "build": "npm run build-umd && npm run build-esm && cp index.js.flow dist/index.umd.js.flow && cp index.js.flow dist/index.esm.js.flow",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "karma start test/karma.config.js"
  },
  "keywords": [
    "crop",
    "image",
    "custom element"
  ],
  "license": "MIT",
  "repository": "github/image-crop-element",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-transform-modules-umd": "^7.2.0",
    "babel-preset-github": "^3.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-plugin-github": "^1.10.0",
    "flow-bin": "^0.98.1",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^6.0.1"
  },
  "dependencies": {}
}
